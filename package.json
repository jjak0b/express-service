{
  "name": "express-service",
  "description": "Express server running inside ServiceWorker",
  "version": "0.0.0-semantic-release",
  "main": "src/service.js",
  "files": [
    "src",
    "examples",
    "dist/index.html",
    "dist/client.js"
  ],
  "scripts": {
    "test": "npm run lint",
    "lint": "standard --verbose --fix *.js src/*.js examples/**/*.js",
    "commit": "commit-wizard",
    "build": "npm run example",
    "example": "npm run example-service && npm run example-client",
    "example-service": "browserify examples/basic/service-worker-server.js -o dist/express-service.js --global-transform=aliasify",
    "example-client": "ncp src/client.js dist/client.js",
    "start": "http-server dist",
    "stand-alone": "node --harmony examples/basic/stand-alone.js",
    "dev-start": "http-server dist -c-1 -p 3007",
    "issues": "git-issues",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjak0b/express-service.git"
  },
  "keywords": [
    "express",
    "server",
    "expressjs",
    "browser",
    "serviceworker"
  ],
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjak0b/express-service/issues"
  },
  "homepage": "https://github.com/jjak0b/express-service#readme",
  "aliasify": {
    "aliases": {
      "http": "stream-http"
    }
  },
  "devDependencies": {
    "aliasify": "^2.1.0",
    "browserify": "17.0.0",
    "git-issues": "1.2.0",
    "http-server": "0.12.3",
    "ncp": "2.0.0",
    "pre-git": "3.17.1",
    "semantic-release": "4.3.5",
    "standard": "8.4.0"
  },
  "dependencies": {
    "check-more-types": "2.10.0",
    "http-proxy": "1.18.1",
    "lazy-ass": "1.3.0"
  },
  "peerDependencies": {
    "express": "^4.16.1",
    "stream-http": "^3.1.1"
  }
}
